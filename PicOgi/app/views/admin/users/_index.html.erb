<div class = "container">
  <div class = "row">
    <div class = "col-md-8 offset-md-2">
      <table class="table table-hover table-inverse" style="margin-top:15px">
        <thead>
          <tr>
            <th scope="col">利用日</th>
            <th scope="col">プロフィール画像</th>
            <th scope="col">ユーザー名</th>
          </tr>
        </thead>
        <tbody>
          <% users.each do |user| %>
          <tr>
            <td><%= user.created_at.strftime('%Y/%m/%d %H:%M:%S') %>から</td>
            <td><%= image_tag  user.get_profile_image(100,100) %></td>
            <td><%= user.nickname %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>