
<% if @user_flag.present? %>
<% if @users.blank? %>
<h2 class = "seach-result">検索結果はありません</h2>
<% else %>
<div class = "user-contents">
  <ul class = "user-list">
    <% @users.each do |user| %>
    <li class = "list"><%= link_to "詳細", user_path(user.id), class: "btn btn-primary" %></li>
    <li class = "list"><%= user.nickname %></li>
    <% end %>
  </ul>
</div>
<% end %>
<% end %>


<% if @subject_flag.present? %>
<% if @subjects.blank? %>
<h2 class = "seach-result">検索結果はありません</h2>
<% else %>
