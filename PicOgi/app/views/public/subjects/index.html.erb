<h1>投稿一覧</h1>
  <div class = 'container'>
    <% @subjects.each do |subject| %>
     <div>
       <%= subject.user.nickname %>
     </div>
     <div>
       <%= image_tag subject.subject_image, size: "100,100" %>
     </div>
     <div>
       <%= subject.title %>
     </div>
     
    <% end %>
  </div>